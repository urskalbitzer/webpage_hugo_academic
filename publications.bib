	keywords = {home range, Animals, Territoriality, Macaca, Behavior, Animal, Social Behavior, Seasons, Ecosystem, food abundance, Diet, Feeding Behavior, delaunay triangulation, fidelity index, Fruit, Homing Behavior, hotspot analysis, Spatio-Temporal Analysis, Thailand},
