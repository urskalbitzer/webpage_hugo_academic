	month = sep,
	year = {2010},
	keywords = {home range, kinship, Social evolution, relatedness, bottlenose dolphin, HWI, Tursiops aduncus},
	pages = {481--486},
	file = {Frère et al. - 2010 - Home range overlap, matrilineal and biparental kin.pdf:/Users/urskalbitzer/Zotero/storage/C3BBHIIV/Frère et al. - 2010 - Home range overlap, matrilineal and biparental kin.pdf:application/pdf;ScienceDirect Snapshot:/Users/urskalbitzer/Zotero/storage/N2QPWTPX/S0003347210002277.html:text/html},
