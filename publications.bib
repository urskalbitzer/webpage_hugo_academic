	urldate = {2019-01-21},
	journal = {Primates},
	author = {Strier, Karen B. and Tabacow, Fernanda P. and de Possamai, Carla B. and Ferreira, Anderson I. G. and Nery, Marcello S. and de Melo, Fabiano R. and Mendes, Sérgio L.},
