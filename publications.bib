@article{gesquiere_measuring_2014,
	title = {Measuring fecal testosterone in females and fecal estrogens in males: {Comparison} of {RIA} and {LC}/{MS}/{MS} methods for wild baboons ({Papio} cynocephalus)},
	volume = {204},
