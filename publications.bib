	keywords = {Time series analysis, Bayesian inference, cross-Validation, pareto Smoothed importance sampling},
	pages = {2499--2523},
	file = {Full Text PDF:/Users/urskalbitzer/Zotero/storage/3ISEMA79/Bürkner et al. - 2020 - Approximate leave-future-out cross-validation for .pdf:application/pdf;Snapshot:/Users/urskalbitzer/Zotero/storage/F6AZBGNB/00949655.2020.html:text/html},
