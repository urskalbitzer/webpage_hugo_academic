	file = {Snapshot:/Users/urskalbitzer/Zotero/storage/8PFDX3EM/978-3-642-22514-7_3.html:text/html;Sussman et al. - 2012 - Beza Mahafaly Special Reserve Long-Term Research .pdf:/Users/urskalbitzer/Zotero/storage/7VERN8ZB/Sussman et al. - 2012 - Beza Mahafaly Special Reserve Long-Term Research .pdf:application/pdf},
