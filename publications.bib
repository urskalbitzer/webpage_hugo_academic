	file = {Carnes et al. - 2011 - Effects of the Distribution of Female Primates on .pdf:/Users/urskalbitzer/Zotero/storage/4JES9JVJ/Carnes et al. - 2011 - Effects of the Distribution of Female Primates on .pdf:application/pdf;PLoS Snapshot:/Users/urskalbitzer/Zotero/storage/JC5ZQM8I/infodoi10.1371journal.pone.html:text/html;Table_S1.doc:/Users/urskalbitzer/Zotero/storage/DSZF6AH6/Table_S1.doc:application/msword},
