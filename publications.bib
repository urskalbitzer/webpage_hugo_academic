	month = jun,
	year = {2017},
	note = {Publisher: Public Library of Science},
	keywords = {Data management, Computer software, Control systems, Metadata, Programming languages, Reproducibility, Software tools, Source code},
	pages = {e1005510},
	file = {Snapshot:/Users/urskalbitzer/Zotero/storage/PG3HYRG4/article.html:text/html;Wilson et al. - 2017 - Good enough practices in scientific computing.pdf:/Users/urskalbitzer/Zotero/storage/IG4TW9XZ/Wilson et al. - 2017 - Good enough practices in scientific computing.pdf:application/pdf},
}

@article{campos_glucocorticoid_2021,
