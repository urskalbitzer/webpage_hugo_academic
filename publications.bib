	pages = {4881--4886},
	file = {Diffenbaugh et al. - 2017 - Quantifying the influence of global warming on unp.pdf:/Users/urskalbitzer/Zotero/storage/XIMV68EU/Diffenbaugh et al. - 2017 - Quantifying the influence of global warming on unp.pdf:application/pdf},
