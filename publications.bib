	pages = {305--315},
	file = {Matuschek et al. - 2017 - Balancing Type I error and power in linear mixed m.pdf:/Users/urskalbitzer/Zotero/storage/W3X8JM4K/Matuschek et al. - 2017 - Balancing Type I error and power in linear mixed m.pdf:application/pdf},
}

