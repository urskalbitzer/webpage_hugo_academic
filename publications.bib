	file = {Kalbitzer et al_2010_Seasonal Changes in Brain Serotonin Transporter Binding in Short Serotonin.pdf:/Users/urskalbitzer/Zotero/storage/2PABVCRM/Kalbitzer et al_2010_Seasonal Changes in Brain Serotonin Transporter Binding in Short Serotonin.pdf:application/pdf},
