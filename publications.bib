	journal = {Journal of Animal Ecology},
	author = {Kranstauber, Bart and Kays, Roland and LaPoint, Scott D. and Wikelski, Martin and Safi, Kamran},
	year = {2012},
	keywords = {home range, GPS, behavioural change, encounter probability, utilization distribution},
	pages = {738--746},
	file = {Full Text PDF:/Users/urskalbitzer/Zotero/storage/L6HX5ENR/Kranstauber et al. - A dynamic Brownian bridge movement model to estima.pdf:application/pdf;Snapshot:/Users/urskalbitzer/Zotero/storage/HJWDWLZY/j.1365-2656.2012.01955.html:text/html},
}

