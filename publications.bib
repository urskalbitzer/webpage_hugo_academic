@incollection{kalbitzer_specialist_2018,
	address = {Cham},
	title = {The {Specialist} {Capuchin}? {Using} {Ecological} {Niche} {Models} to {Compare} {Niche} {Breadth} in {Mesoamerican} {Primates}},
	isbn = {978-3-319-98284-7 978-3-319-98285-4},
	shorttitle = {The {Specialist} {Capuchin}?},
	url = {http://link.springer.com/10.1007/978-3-319-98285-4_15},
	abstract = {The high degree of dietary breadth and flexibility in capuchins (Cebus and Sapajus), coupled with their advanced cognitive abilities, is well documented. Owing to these characteristics, capuchins may be considered highly adaptable generalists, capable of occupying a wide range of habitats. The Panamanian white-­ throated capuchin (Cebus (capucinus) imitator) coexists with several other primate genera in Mesoamerica. Some taxa, such as the mantled howler monkey (Alouatta palliata) and Geoffroy’s spider monkey (Ateles geoffroyi), have broader geographic ranges than C. imitator. This may be due to historical biogeography (e.g. earlier colonization of the region by Ateles and Alouatta) or, alternatively, because some habitats may be less suitable for capuchins. We investigated the latter hypothesis using ecological niche models (ENMs) to predict range extent based on climate and elevation, as well as niche breadth. Our results suggest a narrow climate niche relative to the more wide-ranging Ateles geoffroyi, as well as Alouatta spp. Precipitation in the coldest quarter and temperature seasonality were the two most important climate variables for determining C. imitator habitat suitability. These findings may have implications for capuchins’ ability to persist through environmental changes (climate change and forest degradation, and loss) and warrant consideration in assessing their extinction risk.},
	language = {en},
	urldate = {2020-01-30},
	booktitle = {Primate {Life} {Histories}, {Sex} {Roles}, and {Adaptability}},
	publisher = {Springer International Publishing},
	author = {Johnson, Steig E. and Brown, Kerry A.},
	editor = {Kalbitzer, Urs and Jack, Katharine M.},
	year = {2018},
	doi = {10.1007/978-3-319-98285-4_15},
	pages = {311--329},
	file = {Johnson and Brown - 2018 - The Specialist Capuchin Using Ecological Niche Mo.pdf:/Users/urskalbitzer/Zotero/storage/NYN57LN8/Johnson and Brown - 2018 - The Specialist Capuchin Using Ecological Niche Mo.pdf:application/pdf},
}

@article{ellis_deconstructing_2019,
	title = {Deconstructing sociality: the types of social connections that predict longevity in a group-living primate},
	volume = {286},
	shorttitle = {Deconstructing sociality},
	url = {https://royalsocietypublishing.org/doi/full/10.1098/rspb.2019.1991},
	doi = {10.1098/rspb.2019.1991},
	abstract = {Many species use social interactions to cope with challenges in their environment and a growing number of studies show that individuals which are well-connected to their group have higher fitness than socially isolated individuals. However, there are many ways to be ‘well-connected’ and it is unclear which aspects of sociality drive fitness benefits. Being well-connected can be conceptualized in four main ways: individuals can be socially integrated by engaging in a high rate of social behaviour or having many partners; they can have strong and stable connections to favoured partners; they can indirectly connect to the broader group structure; or directly engage in a high rate of beneficial behaviours, such as grooming. In this study, we use survival models and long-term data in adult female rhesus macaques (Macaca mulatta) to compare the fitness outcomes of multiple measures of social connectedness. Females that maintained strong connections to favoured partners had the highest relative survival probability, as did females well-integrated owing to forming many weak connections. We found no survival benefits to being structurally well-connected or engaging in high rates of grooming. Being well-connected to favoured partners could provide fitness benefits by, for example, increasing the efficacy of coordinated or mutualistic behaviours.},
	number = {1917},
	urldate = {2020-01-29},
	journal = {Proceedings of the Royal Society B: Biological Sciences},
	author = {Ellis, Samuel and Snyder-Mackler, Noah and Ruiz-Lambides, Angelina and Platt, Michael L. and Brent, Lauren J. N.},
	month = dec,
	year = {2019},
	pages = {20191991},
	file = {Ellis et al. - 2019 - Deconstructing sociality the types of social conn.pdf:/Users/urskalbitzer/Zotero/storage/VGSZXL37/Ellis et al. - 2019 - Deconstructing sociality the types of social conn.pdf:application/pdf;rspb20191991_si_001.docx:/Users/urskalbitzer/Zotero/storage/8984LHTI/rspb20191991_si_001.docx:application/vnd.openxmlformats-officedocument.wordprocessingml.document;rspb20191991_si_002.csv:/Users/urskalbitzer/Zotero/storage/26J8KN5K/rspb20191991_si_002.csv:text/csv;rspb20191991_si_003.csv:/Users/urskalbitzer/Zotero/storage/95AGJ9KY/rspb20191991_si_003.csv:text/csv;rspb20191991_si_004.csv:/Users/urskalbitzer/Zotero/storage/8GLAUE6V/rspb20191991_si_004.csv:text/csv;Snapshot:/Users/urskalbitzer/Zotero/storage/VFD6X6AL/rspb.2019.html:text/html},
