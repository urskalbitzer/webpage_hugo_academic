	note = {\_eprint: https://besjournals.onlinelibrary.wiley.com/doi/pdf/10.1111/2041-210X.12528},
	keywords = {dispersal, movement ecology, conditional logistic regression, habitat selection, random walk, redistribution kernel, resource selection, step selection, telemetry, utilisation distribution},
	pages = {619--630},
	file = {Avgar et al. - 2016 - Integrated step selection analysis bridging the g.pdf:/Users/urskalbitzer/Zotero/storage/LPNH7Y6Z/Avgar et al. - 2016 - Integrated step selection analysis bridging the g.pdf:application/pdf;Snapshot:/Users/urskalbitzer/Zotero/storage/8GYMIW45/2041-210X.html:text/html},
