	pmcid = {PMC4410382},
	file = {Kappeler et al. - 2015 - Sociality and health impacts of sociality on dise.pdf:/Users/urskalbitzer/Zotero/storage/CW4XGWGL/Kappeler et al. - 2015 - Sociality and health impacts of sociality on dise.pdf:application/pdf},
}

@article{wal_influence_2012,
	title = {Influence of landscape and social interactions on transmission of disease in a social cervid},
