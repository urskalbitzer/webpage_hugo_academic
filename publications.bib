	journal = {Philosophical Transactions of the Royal Society of London B: Biological Sciences},
	author = {Marco, Moreno Di and Buchanan, Graeme M. and Szantoi, Zoltan and Holmgren, Milena and Marasini, Gabriele Grottolo and Gross, Dorit and Tranquilli, Sandra and Boitani, Luigi and Rondinini, Carlo},
