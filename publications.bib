	keywords = {Papio cynocephalus, Baboons, Reproductive success, longevity, life history},
	pages = {229--241},
	file = {Rhine et al. - 2000 - Lifetime reproductive success, longevity, and repr.pdf:/Users/urskalbitzer/Zotero/storage/GEXC3U6D/Rhine et al. - 2000 - Lifetime reproductive success, longevity, and repr.pdf:application/pdf;Snapshot:/Users/urskalbitzer/Zotero/storage/E2394HBG/abstract\;jsessionid=CBBE37CB7003F726609099E00FE0A016.html:text/html},
