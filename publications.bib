	pages = {93--102},
	file = {Springer Full Text PDF:/Users/urskalbitzer/Zotero/storage/UNHQXFZY/Goldsborough et al. - 2020 - Do chimpanzees (Pan troglodytes) console a bereave.pdf:application/pdf},
}

