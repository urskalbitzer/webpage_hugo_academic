@article{bergstrom_dominance_2010,
	title = {Dominance among female white-faced capuchin monkeys (\textit{{Cebus} capucinus}): hierarchical linearity, nepotism, strength and stability},
	volume = {147},
	issn = {1568-539X},
	shorttitle = {Dominance among female white-faced capuchin monkeys (\textit{{Cebus} capucinus})},
	url = {http://booksandjournals.brillonline.com/content/journals/10.1163/000579510x497283},
	doi = {10.1163/000579510X497283},
	abstract = {Research on Old World primates provided the foundation for understanding competitive strategies resulting from social and ecological pressures. The neotropical primate, \textit{Cebus capucinus} shares many social patterns with Old World cercopithecines (e.g., female philopatry, male dispersal), which may contribute to similar expression of competitive strategies. To clarify the nature of dominance patterns among female white-faced capuchins we examined hierarchical linearity, rank acquisition, matrilineal rank inheritance, hierarchical strength and stability. We collected focal data on 22 adult females (2008) and long-term dominance data (1986–2008) on 33 adult females in Sector Santa Rosa, Costa Rica. Females displayed linear hierarchies based on the direction of dyadic submission. At sexual maturity females quickly acquired rank positions beneath their mother and older sisters. Hierarchies were considered strong based on high proportions of food-related agonism, short latency to detection of hierarchies (21 h/female) and low directional inconsistency scores (\&lt;5\%). Hierarchies were considered stable based on lack of tied submissive interactions (indicative of uncontested rank positions), low rates of rank change (0.510 changes/year), and long-term stability in matrilineal rank order. These findings enhance our understanding of capuchin social systems and how the competitive strategies of white-faced capuchins compare to those of Old World primates.},
	number = {7},
	urldate = {2016-04-28},
	journal = {Behaviour},
	author = {Bergstrom, Mackenzie L. and Fedigan, Linda M.},
	month = jun,
	year = {2010},
	keywords = {Neotropical primates, COMPETITIVE STRATEGIES, FEMALES, RANK INHERITANCE, social behaviour},
	pages = {899--931},
	file = {Bergstrom and Fedigan - 2010 - Dominance among female white-faced capuchin monkey.pdf:/Users/urskalbitzer/Zotero/storage/DD4MHK7K/Bergstrom and Fedigan - 2010 - Dominance among female white-faced capuchin monkey.pdf:application/pdf;Snapshot:/Users/urskalbitzer/Zotero/storage/XRBIB4GC/000579510x497283.html:text/html},
}

