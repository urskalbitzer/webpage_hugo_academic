	pages = {342--353},
	file = {Full Text PDF:/Users/urskalbitzer/Zotero/storage/Z9X8Q74T/Colchero et al. - 2019 - The diversity of population responses to environme.pdf:application/pdf;Snapshot:/Users/urskalbitzer/Zotero/storage/URDUPCA5/ele.html:text/html},
}

@article{strier_status_2019,
