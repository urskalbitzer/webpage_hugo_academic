	keywords = {AIC, BIC, Model averaging, Model selection, Multimodel inference},
	pages = {261--304},
	file = {Burnham and Anderson - 2004 - Multimodel Inference Understanding AIC and BIC in .pdf:/Users/urskalbitzer/Zotero/storage/98BPJG35/Burnham and Anderson - 2004 - Multimodel Inference Understanding AIC and BIC in .pdf:application/pdf;Snapshot:/Users/urskalbitzer/Zotero/storage/699PUTS5/261.html:text/html},
