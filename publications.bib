	month = nov,
	year = {2015},
	keywords = {Reproductive success, follower males, Male competition, male reproductive strategy, one-male groups},
	pages = {501--513},
	file = {Chowdhury et al. - 2015 - The adaptive value of secondary males in the polyg.pdf:/Users/urskalbitzer/Zotero/storage/9MDMPCMA/Chowdhury et al. - 2015 - The adaptive value of secondary males in the polyg.pdf:application/pdf;Snapshot:/Users/urskalbitzer/Zotero/storage/3H7NE9PE/abstract.html:text/html},
