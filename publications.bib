	author = {da Silva, Fabiana Araújo and Canale, Gustavo Rodrigues and Kierulff, Maria Cecília M. and Duarte, Gabriela Teixeira and Paglia, Adriano Pereira and Bernardo, Christine S. S.},
	month = may,
	year = {2016},
	keywords = {Atlantic Forest, extinction vortex, forest loss, minimum viable population, threatened species},
	pages = {n/a--n/a},
	file = {da Silva et al. - 2016 - Hunting, pet trade, and forest size effects on pop.pdf:/Users/urskalbitzer/Zotero/storage/4HBMTFEQ/da Silva et al. - 2016 - Hunting, pet trade, and forest size effects on pop.pdf:application/pdf;Snapshot:/Users/urskalbitzer/Zotero/storage/35QUKIAJ/abstract.html:text/html},
