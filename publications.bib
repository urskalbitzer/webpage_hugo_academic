	file = {Miller et al. - 2020 - Spatiotemporal association patterns in a supergrou.pdf:/Users/urskalbitzer/Zotero/storage/NKZYT5KR/Miller et al. - 2020 - Spatiotemporal association patterns in a supergrou.pdf:application/pdf},
