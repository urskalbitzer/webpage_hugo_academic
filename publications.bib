	urldate = {2019-08-23},
	journal = {American Journal of Primatology},
	author = {Vogel, Erin R. and Janson, C. H.},
	year = {2007},
	keywords = {Cebus capucinus, contest competition, focal tree method, socioecology},
	pages = {533--550},
	file = {Snapshot:/Users/urskalbitzer/Zotero/storage/I6XHTLD5/ajp.html:text/html;Vogel and Janson - 2007 - Predicting the frequency of food-related agonism i.pdf:/Users/urskalbitzer/Zotero/storage/C27U67KG/Vogel and Janson - 2007 - Predicting the frequency of food-related agonism i.pdf:application/pdf},
