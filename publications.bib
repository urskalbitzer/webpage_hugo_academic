	journal = {Animal Cognition},
	author = {Surbeck, Martin and Hohmann, Gottfried},
	month = dec,
	year = {2014},
	keywords = {Evolutionary Biology, Grooming, Behavioral Sciences, Zoology, altruism, Reciprocity, Social bonds, Pan paniscus, Biological market, Emotional mediation hypothesis, Human Physiology, Primates, great apes, Short-term contingency, Tit-for-tat},
	pages = {573--579},
	file = {Snapshot:/Users/urskalbitzer/Zotero/storage/6D6Z8G79/10.html:text/html;Surbeck and Hohmann - 2014 - Social preferences influence the short-term exchan.pdf:/Users/urskalbitzer/Zotero/storage/AF8B7BUF/Surbeck and Hohmann - 2014 - Social preferences influence the short-term exchan.pdf:application/pdf},
