	author = {Sussman, Robert W. and Richard, Alison F. and Ratsirarson, Joelisoa and Sauther, Michelle L. and Brockman, Diane K. and Gould, Lisa and Lawler, Richard and Cuozzo, Frank P.},
	year = {2012},
	doi = {10.1007/978-3-642-22514-7_3},
