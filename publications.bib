	keywords = {Body Weight, Animals, Papio, Testosterone, Female, Male, Animals, Laboratory, Anthropometry, Dehydroepiandrosterone, Insulin-Like Growth Factor Binding Protein 3, Insulin-Like Growth Factor I, Osteocalcin, Sex Characteristics},
