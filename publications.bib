@article{zinner_cycle_1994,
	title = {Cycle synchrony and probability of conception in female hamadryas {baboonsPapio} hamadryas},
	volume = {35},
	issn = {0340-5443, 1432-0762},
	url = {http://link.springer.com/article/10.1007/BF00167957},
	doi = {10.1007/BF00167957},
	abstract = {We investigated the factors affecting conception in a captive population ofPapio hamadryas. We collected data on reproductive states from 16 females over 12 years. Probability of conception was related to the outcome of the immediately preceding pregnancy and the fate of the preceding offspring. After abortions or when an infant survived more than 6 months, a female needed more cycles to conceive than when an infant died within the first 6 months post partum. The degree of estrus synchrony within, but not between, one-male units influenced the probability of conception. Females experiencing conceptive estrus showed less synchrony than those experiencing nonconceptive estrus. The number of females simultaneously in estrus within a one-male unit was negatively correlated with the probability of conception. Within our captive population, group size and male and female age had no influence on the probability of conception. The results indicate that sperm may be a limited resource in the one-male reproductive units of hamadryas baboons. Female-female competition for conception may exist and should influence the demography of one-male units.},
	language = {en},
	number = {3},
	urldate = {2015-08-05},
	journal = {Behavioral Ecology and Sociobiology},
	author = {Zinner, Dietmar and Schwibbe, Michael H. and Kaumanns, Werner},
	month = sep,
	year = {1994},
	keywords = {Zoology, Papio hamadryas, Behavioural Sciences, Female-female competition, One-male units, Probability of conception, Synchrony of estrus},
	pages = {175--183},
	file = {Snapshot:/Users/urskalbitzer/Zotero/storage/SB2ZNSV9/10.html:text/html;Zinner et al. - 1994 - Cycle synchrony and probability of conception in f.pdf:/Users/urskalbitzer/Zotero/storage/9TZBFGAC/Zinner et al. - 1994 - Cycle synchrony and probability of conception in f.pdf:application/pdf},
}

@article{petelle_heritability_2015,
	title = {Heritability and genetic correlations of personality traits in a wild population of yellow-bellied marmots ({Marmota} flaviventris)},
	copyright = {This article is protected by copyright. All rights reserved.},
	issn = {1420-9101},
	url = {http://onlinelibrary.wiley.com/doi/10.1111/jeb.12700/abstract},
	doi = {10.1111/jeb.12700},
	abstract = {Describing and quantifying animal personality is now an integral part of behavioural studies because individually distinctive behaviours have ecological and evolutionary consequences. Yet, to fully understand how personality traits may respond to selection, one must understand the underlying heritability and genetic correlations between traits. Previous studies have reported a moderate degree of heritability of personality traits but few of these studies have either been conducted in the wild or estimated the genetic correlations between personality traits. Estimating the additive genetic variance and covariance in the wild is crucial to understand the evolutionary potential of behavioural traits. Enhanced environmental variation could reduce heritability and genetic correlations thus leading to different evolutionary predictions. We estimated the additive genetic variance and covariance of docility in the trap, sociability (mirror image stimulation), and exploration and activity in two different contexts (open-field and mirror image simulation experiments) in a wild population of yellow-bellied marmots (Marmota flaviventris). We estimated both heritability of behaviours and of personality traits and found non-zero additive genetic variance in these traits. We also found non-zero maternal, permanent environment, and year effects. Finally, we found four phenotypic correlations between traits, and one positive genetic correlation between activity in the open-field test and sociability. We also found permanent environment correlations between activity in both tests and docility and exploration in the MIS test. This is one of a handful of studies to adopt a quantitative genetic approach to explain variation in personality traits in the wild, and thus, provides important insights into the potential variance available for selection. This article is protected by copyright. All rights reserved.},
	language = {en},
	urldate = {2015-07-29},
	journal = {Journal of Evolutionary Biology},
	author = {Petelle, Matthew B. and Martin, Julien G.A. and Blumstein, Daniel T.},
	month = jul,
	year = {2015},
	keywords = {Heritability, behavioral syndromes, personality, quantitative genetics},
	pages = {n/a--n/a},
	file = {Petelle et al. - 2015 - Heritability and genetic correlations of personali.pdf:/Users/urskalbitzer/Zotero/storage/GFZ9T3VS/Petelle et al. - 2015 - Heritability and genetic correlations of personali.pdf:application/pdf;Snapshot:/Users/urskalbitzer/Zotero/storage/V6ST5UIX/abstract.html:text/html},
}

@article{choi_novel_2014,
	title = {Novel variable number of tandem repeats of gibbon {MAOA} gene and its evolutionary significance},
	volume = {57},
	issn = {0831-2796},
	url = {http://www.nrcresearchpress.com/doi/abs/10.1139/gen-2014-0065},
	doi = {10.1139/gen-2014-0065},
	abstract = {Variable number of tandem repeats (VNTRs) are scattered throughout the primate genome, and genetic variation of these VNTRs have been accumulated during primate radiation. Here, we analyzed VNTRs upstream of the monoamine oxidase A (MAOA) gene in 11 different gibbon species. An abundance of truncated VNTR sequences and copy number differences were observed compared to those of human VNTR sequences. To better understand the biological role of these VNTRs, a luciferase activity assay was conducted and results indicated that selected VNTR sequences of the MAOA gene from human and three different gibbon species (Hylobates klossii, Hylobates lar, and Nomascus concolor) showed silencing ability. Together, these data could be useful for understanding the evolutionary history and functional significance of MAOA VNTR sequences in gibbon species., Les répétitions en tandem en nombre variable (VNTR) sont dispersées dans les génomes des primates et la variation à ces VNTR s’est accumulée au cours de l’évolution des primates. Dans ce travail, les auteurs ont analysé les VNTR en amont du gène monoamine oxydase A (MAOA) chez 11 espèces de gibbons. Un grand nombre de séquences VNTR tronquées et de différences dans le nombre de répétitions ont été observées par rapport à ce qu’on observe chez les VNTR humains. Afin de mieux comprendre le rôle biologique de ces VNTR, un rapporteur faisant appel à la luciférase a été employé et a permis de constater que certaines séquences VNTR du gène MAOA chez l’humain et chez trois espèces de gibbons (Hylobates klossii, Hylobates lar et Nomascus concolor) pouvaient entraîner une inactivation du gène. Ensemble, ces données pourraient s’avérer utiles pour décrypter l’historique évolutif et la signification fonctionnelle des séquences MAOA VNTR chez les espèces de gibbons. [Traduit par la Rédaction]},
	number = {8},
	urldate = {2015-07-25},
	journal = {Genome},
	author = {Choi, Yuri and Jung, Yi-Deun and Ayarpadikannan, Selvam and Koga, Akihiko and Imai, Hiroo and Hirai, Hirohisa and Roos, Christian and Kim, Heui-Soo},
	month = aug,
	year = {2014},
	pages = {427--432},
	file = {Choi et al. - 2014 - Novel variable number of tandem repeats of gibbon .pdf:/Users/urskalbitzer/Zotero/storage/F8DXZB8Z/Choi et al. - 2014 - Novel variable number of tandem repeats of gibbon .pdf:application/pdf;NRC Research Press Snapshot:/Users/urskalbitzer/Zotero/storage/TJTQN784/gen-2014-0065.html:text/html},
}

@article{malki_genes_2014,
	title = {Genes and {Gene} {Networks} {Implicated} in {Aggression} {Related} {Behaviour}},
	volume = {15},
	issn = {1364-6745, 1364-6753},
	url = {http://link.springer.com/article/10.1007/s10048-014-0417-x},
	doi = {10.1007/s10048-014-0417-x},
	abstract = {Aggressive behaviour is a major cause of mortality and morbidity. Despite of moderate heritability estimates, progress in identifying the genetic factors underlying aggressive behaviour has been limited. There are currently three genetic mouse models of high and low aggression created using selective breeding. This is the first study to offer a global transcriptomic characterization of the prefrontal cortex across all three genetic mouse models of aggression. A systems biology approach has been applied to transcriptomic data across the three pairs of selected inbred mouse strains (Turku Aggressive (TA) and Turku Non-Aggressive (TNA), Short Attack Latency (SAL) and Long Attack Latency (LAL) mice and North Carolina Aggressive (NC900) and North Carolina Non-Aggressive (NC100)), providing novel insight into the neurobiological mechanisms and genetics underlying aggression. First, weighted gene co-expression network analysis (WGCNA) was performed to identify modules of highly correlated genes associated with aggression. Probe sets belonging to gene modules uncovered by WGCNA were carried forward for network analysis using ingenuity pathway analysis (IPA). The RankProd non-parametric algorithm was then used to statistically evaluate expression differences across the genes belonging to modules significantly associated with aggression. IPA uncovered two pathways, involving NF-kB and MAPKs. The secondary RankProd analysis yielded 14 differentially expressed genes, some of which have previously been implicated in pathways associated with aggressive behaviour, such as Adrbk2. The results highlighted plausible candidate genes and gene networks implicated in aggression-related behaviour.},
	language = {en},
	number = {4},
	urldate = {2015-07-17},
	journal = {neurogenetics},
	author = {Malki, Karim and Pain, Oliver and Rietz, Ebba Du and Tosto, Maria Grazia and Paya-Cano, Jose and Sandnabba, Kenneth N. and Boer, Sietse de and Schalkwyk, Leonard C. and Sluyter, Frans},
	month = aug,
	year = {2014},
	keywords = {Human Genetics, Aggression, Molecular Medicine, NC900/NC100, Neurosciences, RankProd, SAL/LAL, TA/TNA, WGCNA},
	pages = {255--266},
	file = {Malki et al. - 2014 - Genes and Gene Networks Implicated in Aggression R.pdf:/Users/urskalbitzer/Zotero/storage/54FZDQ43/Malki et al. - 2014 - Genes and Gene Networks Implicated in Aggression R.pdf:application/pdf;Snapshot:/Users/urskalbitzer/Zotero/storage/SZ2IXUSJ/s10048-014-0417-x.html:text/html},
}

