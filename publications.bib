	journal = {Proceedings of the National Academy of Sciences},
	author = {Nathan, Ran and Getz, Wayne M. and Revilla, Eloy and Holyoak, Marcel and Kadmon, Ronen and Saltz, David and Smouse, Peter E.},
	month = dec,
	year = {2008},
	pmid = {19060196},
	note = {Publisher: National Academy of Sciences
Section: Perspective},
	keywords = {dispersal, foraging, migration, motion capacity, navigation capacity},
	pages = {19052--19059},
	file = {Nathan et al. - 2008 - A movement ecology paradigm for unifying organisma.pdf:/Users/urskalbitzer/Zotero/storage/NXQHRMQA/Nathan et al. - 2008 - A movement ecology paradigm for unifying organisma.pdf:application/pdf;Snapshot:/Users/urskalbitzer/Zotero/storage/P2KTE22V/19052.html:text/html},
