	number = {6131},
	urldate = {2017-04-12},
	journal = {Science},
	author = {Waal, Erica van de and Borgeaud, Christèle and Whiten, Andrew},
	month = apr,
	year = {2013},
	pmid = {23620053},
	pages = {483--485},
	file = {Snapshot:/Users/urskalbitzer/Zotero/storage/U57AIWIM/483.html:text/html;Waal et al. - 2013 - Potent Social Learning and Conformity Shape a Wild.pdf:/Users/urskalbitzer/Zotero/storage/P83BXUWS/Waal et al. - 2013 - Potent Social Learning and Conformity Shape a Wild.pdf:application/pdf},
}

@article{christakis_friendship_2014,
	title = {Friendship and natural selection},
	volume = {111},
	issn = {0027-8424, 1091-6490},
	url = {http://www.pnas.org/content/111/Supplement_3/10796},
	doi = {10.1073/pnas.1400825111},
	abstract = {More than any other species, humans form social ties to individuals who are neither kin nor mates, and these ties tend to be with similar people. Here, we show that this similarity extends to genotypes. Across the whole genome, friends’ genotypes at the single nucleotide polymorphism level tend to be positively correlated (homophilic). In fact, the increase in similarity relative to strangers is at the level of fourth cousins. However, certain genotypes are also negatively correlated (heterophilic) in friends. And the degree of correlation in genotypes can be used to create a “friendship score” that predicts the existence of friendship ties in a hold-out sample. A focused gene-set analysis indicates that some of the overall correlation in genotypes can be explained by specific systems; for example, an olfactory gene set is homophilic and an immune system gene set is heterophilic, suggesting that these systems may play a role in the formation or maintenance of friendship ties. Friends may be a kind of “functional kin.” Finally, homophilic genotypes exhibit significantly higher measures of positive selection, suggesting that, on average, they may yield a synergistic fitness advantage that has been helping to drive recent human evolution.},
	language = {en},
	number = {Supplement 3},
	urldate = {2017-04-12},
	journal = {Proceedings of the National Academy of Sciences},
	author = {Christakis, Nicholas A. and Fowler, James H.},
	month = jul,
	year = {2014},
	pmid = {25024208},
	keywords = {genetics, kinship detection, Social networks},
	pages = {10796--10801},
	file = {Full Text PDF:/Users/urskalbitzer/Zotero/storage/9SQWIJ27/Christakis and Fowler - 2014 - Friendship and natural selection.pdf:application/pdf;Snapshot:/Users/urskalbitzer/Zotero/storage/ED7UU8HZ/10796.html:text/html},
}

