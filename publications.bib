@article{hogan_group_2019,
	title = {Group versus population level demographics: {An} analysis of comparability using long term data on wild white-faced capuchin monkeys (\textit{{Cebus} capucinus imitator})},
	volume = {81},
	copyright = {© 2019 Wiley Periodicals, Inc.},
	issn = {1098-2345},
