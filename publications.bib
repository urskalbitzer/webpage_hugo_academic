	year = {2018},
	keywords = {C-peptide, Alouatta palliata, carrying, infant age, vigilance},
	pages = {178--184},
	file = {Dias et al. - 2018 - Maternal condition and maternal investment during .pdf:/Users/urskalbitzer/Zotero/storage/6FEWJEL5/Dias et al. - 2018 - Maternal condition and maternal investment during .pdf:application/pdf;Snapshot:/Users/urskalbitzer/Zotero/storage/WMM5A6ES/ajpa.html:text/html},
