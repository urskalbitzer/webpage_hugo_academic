@article{palombit_infanticide_1999,
	title = {Infanticide and the evolution of pair bonds in nonhuman primates},
	volume = {7},
	copyright = {Copyright © 1999 Wiley-Liss, Inc.},
	issn = {1520-6505},
	url = {http://onlinelibrary.wiley.com.ezproxy.lib.ucalgary.ca/doi/10.1002/(SICI)1520-6505(1999)7:4<117::AID-EVAN2>3.0.CO;2-O/abstract},
