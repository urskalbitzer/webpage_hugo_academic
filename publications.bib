@article{farine_estimating_2015,
	title = {Estimating uncertainty and reliability of social network data using {Bayesian} inference},
	volume = {2},
	copyright = {. © 2015 The Authors. Published by the Royal Society under the terms of the Creative Commons Attribution License http://creativecommons.org/licenses/by/4.0/, which permits unrestricted use, provided the original author and source are credited.},
	issn = {2054-5703},
	url = {http://rsos.royalsocietypublishing.org/content/2/9/150367},
	doi = {10.1098/rsos.150367},
	abstract = {Social network analysis provides a useful lens through which to view the structure of animal societies, and as a result its use is increasingly widespread. One challenge that many studies of animal social networks face is dealing with limited sample sizes, which introduces the potential for a high level of uncertainty in estimating the rates of association or interaction between individuals. We present a method based on Bayesian inference to incorporate uncertainty into network analyses. We test the reliability of this method at capturing both local and global properties of simulated networks, and compare it to a recently suggested method based on bootstrapping. Our results suggest that Bayesian inference can provide useful information about the underlying certainty in an observed network. When networks are well sampled, observed networks approach the real underlying social structure. However, when sampling is sparse, Bayesian inferred networks can provide realistic uncertainty estimates around edge weights. We also suggest a potential method for estimating the reliability of an observed network given the amount of sampling performed. This paper highlights how relatively simple procedures can be used to estimate uncertainty and reliability in studies using animal social network analysis.},
	language = {en},
	number = {9},
	urldate = {2015-10-06},
	journal = {Royal Society Open Science},
	author = {Farine, Damien R. and Strandburg-Peshkin, Ariana},
	month = sep,
	year = {2015},
	pages = {150367},
	file = {Farine and Strandburg-Peshkin - 2015 - Estimating uncertainty and reliability of social n.pdf:/Users/urskalbitzer/Zotero/storage/X4HFBC3J/Farine and Strandburg-Peshkin - 2015 - Estimating uncertainty and reliability of social n.pdf:application/pdf;rsos150367_review_history.pdf:/Users/urskalbitzer/Zotero/storage/AW27UYDQ/rsos150367_review_history.pdf:application/pdf;rsos150367supp1.pdf:/Users/urskalbitzer/Zotero/storage/ZYQ6AKUH/rsos150367supp1.pdf:application/pdf;rsos150367supp2.r:/Users/urskalbitzer/Zotero/storage/AXHNGW76/rsos150367supp2.r:text/plain;rsos150367supp3.r:/Users/urskalbitzer/Zotero/storage/C6349YDZ/rsos150367supp3.r:text/plain;rsos150367supp4.r:/Users/urskalbitzer/Zotero/storage/DYN2QSGU/rsos150367supp4.r:text/plain;Snapshot:/Users/urskalbitzer/Zotero/storage/4E3RAMGV/150367.html:text/html},
