	pages = {279--306},
	file = {Alberts_Altmann_1995_Balancing Costs and Opportunities.pdf:/Users/urskalbitzer/Zotero/storage/EPA9XCQP/Alberts_Altmann_1995_Balancing Costs and Opportunities.pdf:application/pdf},
